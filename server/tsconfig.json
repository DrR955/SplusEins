{
  "compilerOptions": {
  "module": "ESNext",
  "moduleResolution": "Bundler",
    "target": "ES2020",
    "sourceMap": true,
    "lib": ["es2017", "es2018", "es2019", "esnext"],
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
    },
    "strict": true,
    // TODO remove these
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": false,
  },
  "exclude": [
    "node_modules"
  ]
}
